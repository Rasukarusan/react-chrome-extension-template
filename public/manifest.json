{
  "manifest_version": 2,
  "version": "0.0.1",
  "short_name": "react-chrome-extemsion",
  "name": "sample",
  "description": "sample",
  "content_scripts": [
    {
      "matches": ["http://*/*", "https://*/*"],
      "js": ["contents.js"]
    }
  ],
  "background": {
    "scripts": [
      "background.js"
    ]
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
